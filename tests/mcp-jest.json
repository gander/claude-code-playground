{
	"server": {
		"command": "node",
		"args": ["./dist/index.js"],
		"transport": "stdio"
	},
	"tests": {
		"tools": {
			"flat_to_json": {
				"args": {
					"tags": "test_value"
				},
				"expect": "content && content.length > 0"
			},
			"get_preset_details": {
				"args": {
					"presetId": "test-id-123"
				},
				"expect": "content && content.length > 0"
			},
			"get_tag_values": {
				"args": {
					"tagKey": "test_value"
				},
				"expect": "content && content.length > 0"
			},
			"json_to_flat": {
				"args": {
					"tags": "test_value"
				},
				"expect": "content && content.length > 0"
			},
			"search_presets": {
				"args": {
					"keyword": "test_value",
					"limit": 50,
					"geometry": "point"
				},
				"expect": "content && content.length > 0"
			},
			"search_presets:empty-geometry": {
				"args": {
					"keyword": "test_value",
					"limit": 50,
					"geometry": ""
				},
				"expect": "exists"
			},
			"search_tags": {
				"args": {
					"keyword": "test_value",
					"limit": 50
				},
				"expect": "content && content.length > 0"
			},
			"suggest_improvements": {
				"args": {
					"tags": "test_value"
				},
				"expect": "content && content.length > 0"
			},
			"validate_tag": {
				"args": {
					"key": "test_value",
					"value": "test_value"
				},
				"expect": "content && content.length > 0"
			},
			"validate_tag_collection": {
				"args": {
					"tags": "test_value"
				},
				"expect": "content && content.length > 0"
			}
		},
		"timeout": 30000
	}
}
