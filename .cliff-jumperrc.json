{
	"$schema": "https://raw.githubusercontent.com/favware/cliff-jumper/main/assets/cliff-jumper.schema.json",
	"changelogPrependFile": "./CHANGELOG.md",
	"commitMessageTemplate": "chore(release): release {{full-name}}@{{new-version}}",
	"createRelease": false,
	"githubRelease": false,
	"install": false,
	"name": "osm-tagging-schema-mcp",
	"org": "gander-tools",
	"packagePath": ".",
	"prBody": "This PR was automatically generated by cliff-jumper\n\n## Changes\n\n{{changelog}}",
	"prBranchTemplate": "release/v{{new-version}}",
	"prTitle": "release: v{{new-version}}",
	"push": true,
	"pushTag": false,
	"skipChangelog": false,
	"skipTag": false,
	"tagTemplate": "v{{new-version}}",
	"usePrBranch": true,
	"verbose": false
}
